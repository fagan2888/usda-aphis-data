<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="2.0">
	<xsd:annotation>
		<xsd:documentation>
		Developed by :		SAIC
		Point of Contact :	Jae Jung
		Version History:	Version 1.0 - 30-Sept-2005 by John Sun and Jae Jung
							Version 1.1 - 21-Feb-2006 by Jae Jung
								- Removed redundant documentation comments. (SIR 3312)
							Version 1.1.1 - 22-Mar-2006 by Jae Jung
								- Added minOccurs="0" for optional element and minOccurs="1" for required element.
								The change will allow the user to have an option to NOT include defined field 
								elements where minOccurs="0".(SIR 3410)
								- Defined maxLength to ALL data elements to prevent users from entering data longer
								than the allowed length in the database. (SIR 3410)
							Version 1.2 - 21-June-2006 by Jae Jung
								- Added a new element AdditionalInformation (SIR 2831)
							Version 1.3 - 20-March-2007 by Jae Jung
								- Added 5 new data elements for element ReleaseLocationDataType (CRM 4972)
							Version 1.3 - 22-May-2007 by Jae Jung
								- Only allow maximum of 50 locations per Origination Point, Destination, and Release Site
								for Interstate Movement, Release Site, and Interstate Movement/Release Site (CRM 5247)
							Version 1.3.1 - 20-Sep-2007 by Erin Le
								- CRM 5413: Change the existing constraint on number of locations per application to 200 in any combination of location types (origin, destination, and release) for all movement types exclusive Importation.
							Version 2.0 - 21-Sep-2009 by Bill Leeper
								- CQ 218/219: Adding new Unique ID field and multiple GPS fields
		</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_Notification
		Description : 	Introduction of Genetically Engineered Plants and Microorganisms
			</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="Submission" type="SubmissionType"/>
	<xsd:complexType name="SubmissionType">
		<xsd:sequence>
			<xsd:element name="Applicant" type="ApplicantDataType"/>
			<xsd:element name="Application" type="ApplicationDataType" maxOccurs="unbounded">
				<xsd:key name="UniqueLocationUniqueId">
					<xsd:selector xpath=".//ReleaseLocation"/>
					<xsd:field xpath="LocationUniqueID"/>
				</xsd:key>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_Notification_Details
		Description : 	Introduction of Genetically Engineered Plants and Microorganisms
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ApplicationDataType">
		<xsd:sequence>
			<xsd:element name="ApplicantReferenceNumber" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="IntroductionType" type="IntroductionType"/>
			<xsd:element name="ApplicationCBIFlag" type="YesNoIndicatorDataType"/>
			<xsd:element name="CBIJustificationStatement" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AdditionalInformation" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Article" type="ArticleDataType"/>
			<xsd:element name="PhenotypicDesignation" type="PhenotypicDesignationDataType" maxOccurs="unbounded"/>
			<xsd:element name="ImportationDetails" type="ImportationDetails" minOccurs="0"/>
			<xsd:element name="InterstateDetails" type="InterstateDetails" minOccurs="0"/>
			<xsd:element name="ReleaseDetails" type="ReleaseDetails" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_LOCATION
		Description : 	
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ImportationDetails">
		<xsd:sequence>
			<xsd:element name="ImportationPointofOrigin" type="ImportationPOOLocationDataType"/>
			<xsd:element name="ImportationDestination" type="DestinationLocationDataType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InterstateDetails">
		<xsd:sequence>
			<xsd:element name="InterstateMovementOriginationPoint" type="ExtLocationDataType" maxOccurs="unbounded"/>
			<xsd:element name="InterstateMovementDestination" type="DestinationLocationDataType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ReleaseDetails">
		<xsd:sequence>
			<xsd:element name="ReleaseLocation" type="ReleaseLocationDataType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImportationPOOLocationDataType">
		<xsd:complexContent>
			<xsd:extension base="ImpLocationComponentsDataType">
				<xsd:sequence>
					<xsd:element name="LocationCountry">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="50"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Contact" type="ContactPersonDataType" minOccurs="0" maxOccurs="3"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExtLocationDataType">
		<xsd:complexContent>
			<xsd:extension base="LocationComponentsDataType">
				<xsd:sequence>
					<xsd:element name="LocationTownshipIsland" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="0"/>
								<xsd:maxLength value="255"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="LocationState" type="RequiredStateDataType"/>
					<xsd:element name="Contact" type="ContactPersonDataType" minOccurs="0" maxOccurs="3"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- Release Location -->
	<xsd:complexType name="ReleaseLocationDataType">
		<xsd:complexContent>
			<xsd:extension base="LocationComponentsDataType">
				<xsd:sequence>
					<xsd:element name="LocationTownshipIsland" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="0"/>
								<xsd:maxLength value="255"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="LocationState" type="RequiredStateDataType"/>
					<xsd:element name="LocationUniqueID">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="52"/>
								<xsd:pattern value="[a-zA-Z0-9'\-']{1,50}|\[[a-zA-Z0-9'\-']{1,50}\]"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="GPSCoordinates">
						<xsd:complexType>
							<xsd:sequence maxOccurs="6">
								<xsd:element name="GPSLatitude">
									<xsd:simpleType>
										<xsd:restriction base="xsd:float">
											<xsd:minInclusive value="-90.0"/>
											<xsd:maxInclusive value="90.0"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>
								<xsd:element name="GPSLongitude">
									<xsd:simpleType>
										<xsd:restriction base="xsd:float">
											<xsd:minInclusive value="-180"/>
											<xsd:maxInclusive value="180"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:element>
							</xsd:sequence>
							<xsd:attribute name="CBI" type="xsd:boolean" use="required"/>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="ReleaseSiteHistory">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
								<xsd:maxLength value="1000"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="CriticalHabitatFlag" type="YesNoIndicatorDataType"/>
					<xsd:element name="CriticalHabitatSpecies" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="0"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="CriticalHabitatEffectAnalysis" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="0"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<!-- END: CRM 4972 -->
					<xsd:element name="IntroductionRelease" type="IntroductionReleaseDataType"/>
					<xsd:element name="Contact" type="ContactPersonDataType" minOccurs="0" maxOccurs="3"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DestinationLocationDataType">
		<xsd:complexContent>
			<xsd:extension base="LocationComponentsDataType">
				<xsd:sequence>
					<xsd:element name="LocationTownshipIsland" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="0"/>
								<xsd:maxLength value="255"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="LocationState" type="RequiredStateDataType"/>
					<xsd:element name="IntroductionMovement" type="IntroductionImportDataType"/>
					<xsd:element name="Contact" type="ContactPersonDataType" minOccurs="0" maxOccurs="3"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ************************************************************************************* -->
	<!-- START: Applicant XML definition                                                       -->
	<!-- ************************************************************************************* -->
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_APPLICANT 
		Description : 	Applicant Information. The person responsible for the information provided in the notification.  Applicants must have a U.S. address.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ApplicantDataType">
		<xsd:sequence>
			<xsd:element name="AppTitle" type="TitleType" minOccurs="0"/>
			<xsd:element name="AppFirstName">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppMiddleName" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppLastName">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OrganizationName">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="150"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OrganizationUniqueID" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Position" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppContDetails" type="AppContDetailDataType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_APPLICANT
		Description : 	Address and Phone Details
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="AppContDetailDataType">
		<xsd:sequence>
			<xsd:element name="AppAddressLine1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppAddressLine2" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppAddressLine3" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppAddressLine4" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppCountyProvince" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppCity">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppTownshipIsland" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppState" type="RequiredStateDataType"/>
			<xsd:element name="AppPostalCode">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppEmail1">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="200"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppEmail2" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="200"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppDayTelephone">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppDayTelephoneExtension" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppFaxTelephone" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppAlternateTelephone" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AppAlternateTelephoneExtension" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ************************************************************************************* -->
	<!-- END: Applicant XML definition                                                         -->
	<!-- ************************************************************************************* -->
	<!-- ************************************************************************************* -->
	<!-- START: Article XML definition                                                         -->
	<!-- ************************************************************************************* -->
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_ARTICLE
		Description : 	Article Information. The regulated item that is being imported, moved from state to state, or released to the environment.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ArticleDataType">
		<xsd:sequence>
			<xsd:element name="ScientificName">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CommonName" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CultivarBreedingLine" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="500"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ************************************************************************************* -->
	<!-- END: Article XML definition                                                           -->
	<!-- ************************************************************************************* -->
	<!-- ************************************************************************************* -->
	<!-- START: Phenotypic Designation XML definition                                          -->
	<!-- ************************************************************************************* -->
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_PhenotypicDesignation 
		Description : 	PhenotypicDesignation Information. A collection of transformed lines from a single given construct
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="PhenotypicDesignationDataType">
		<xsd:sequence>
			<xsd:element name="PhenotypicDesignationName" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Lines" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Construct">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="64"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ModeofTransformation">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="250"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Genotype" type="GenotypeDataType" maxOccurs="unbounded"/>
			<xsd:element name="Phenotypes" type="PhenotypeDataType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ************************************************************************************* -->
	<!-- END: Phenotypic Designation XML definition                                            -->
	<!-- ************************************************************************************* -->
	<!-- ************************************************************************************* -->
	<!-- START: Phenotype XML definition                                                       -->
	<!-- ************************************************************************************* -->
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_PHENOTYPE
		Description : 	The category of modification and quality/property of the regulated item.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="PhenotypeDataType">
		<xsd:sequence>
			<xsd:element name="PhenotypeCategory" type="PhenotypeCategory"/>
			<xsd:element name="Phenotype">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="PhenotypeCategory">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AP"/>   <!-- AP - Agronomic Properties -->
			<xsd:enumeration value="BR"/>   <!-- BR - Bacterial Resistance -->
			<xsd:enumeration value="FR"/>   <!-- FR - Fungal Resistance -->
			<xsd:enumeration value="HT"/>   <!-- HT - Herbicide Tolerance -->
			<xsd:enumeration value="IR"/>   <!-- IR - Insect Resistance -->
			<xsd:enumeration value="MG"/>   <!-- MG - Marker Gene -->
			<xsd:enumeration value="NR"/>   <!-- NR - Nematode Resistance -->
			<xsd:enumeration value="OO"/>   <!-- OO - Other -->
			<xsd:enumeration value="PQ"/>   <!-- PQ - Product Quality -->
			<xsd:enumeration value="VR"/>   <!-- VR - Virus Resistance -->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ************************************************************************************* -->
	<!-- END: Phenotype XML definition                                                         -->
	<!-- ************************************************************************************* -->
	<!-- ************************************************************************************* -->
	<!-- START: Genotype XML definition                                                        -->
	<!-- ************************************************************************************* -->
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_GENOTYPE
		Description : 	Genotype Information. The gene type (gene of interest, gene silencer, selectable marker, screenable marker) in association with a group of construct components.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="GenotypeDataType">
		<xsd:sequence>
			<xsd:element name="GenotypeType">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ConstructComponent" type="ConstructComponentDataType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ************************************************************************************* -->
	<!-- END: Genotype XML definition                                                          -->
	<!-- ************************************************************************************* -->
	<!-- ************************************************************************************* -->
	<!-- START: Construct Component XML definition                                             -->
	<!-- ************************************************************************************* -->
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_CONSTRUCT_COMPONENT
		Description : 	Construct Component Information. An individual promoter, gene, terminator, etc. defined by its construct component type; construct component name or function, and donor.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ConstructComponentDataType">
		<xsd:sequence>
			<xsd:element name="ConstructComponentType">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="64"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ConstructComponentNameorFunction">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ConstructComponentDescription">
