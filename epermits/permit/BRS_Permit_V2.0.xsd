	<!-- END: Phenotype XML definition                                                         -->
	<!-- ************************************************************************************* -->
	<!-- ************************************************************************************* -->
	<!-- START: Genotype XML definition                                                        -->
	<!-- ************************************************************************************* -->
	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_GENOTYPE
		Description : 	Genotype Information. The gene type (gene of interest, gene silencer, selectable marker, screenable marker) in association with a group of construct components.
		</xsd:documentation>
