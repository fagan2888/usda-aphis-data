				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="PhenotypeCategory">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AP"/>   <!-- AP - Agronomic Properties -->
			<xsd:enumeration value="BR"/>   <!-- BR - Bacterial Resistance -->
			<xsd:enumeration value="FR"/>   <!-- FR - Fungal Resistance -->
			<xsd:enumeration value="HT"/>   <!-- HT - Herbicide Tolerance -->
			<xsd:enumeration value="IR"/>   <!-- IR - Insect Resistance -->
			<xsd:enumeration value="MG"/>   <!-- MG - Marker Gene -->
			<xsd:enumeration value="NR"/>   <!-- NR - Nematode Resistance -->
			<xsd:enumeration value="OO"/>   <!-- OO - Other -->
			<xsd:enumeration value="PQ"/>   <!-- PQ - Product Quality -->
			<xsd:enumeration value="VR"/>   <!-- VR - Virus Resistance -->
		</xsd:restriction>
	</xsd:simpleType>
