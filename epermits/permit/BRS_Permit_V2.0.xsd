	<xsd:annotation>
		<xsd:documentation>
		Schema Name : 	BRS_DESIGN_PROTOCOL
		Description : 	Design Protocol Information. A detailed description of the location and processes, procedures, and safeguards in preventing contamination, release, and dissemination in the production of the article.
		</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="DesignProtocolDataType">
		<xsd:sequence>
			<xsd:element name="ProposedExperiment" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DestRelLocation" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DestRelPrevention" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DispositionMethod" type="DispositionMethodList" minOccurs="0"/>
			<xsd:element name="FinalDisposition" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ************************************************************************************* -->
	<!-- END: Design Protocol XML definition                                                           -->
